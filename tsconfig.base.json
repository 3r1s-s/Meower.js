{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */

    /* Projects */

    /* Language and Environment */
    "target": "ES2021",        
                              
    "lib": [
      "ES2023",
      "DOM"
    ],                                                   
        "sourceMap": true,
        "sourceRoot": "src",
        "declaration": true,

    /* JavaScript Support */
    "allowJs": true,                                     
    "checkJs": true,                                     

    /* Emit */
    "outDir": "./dist/",                                 

    /* Interop Constraints */
    "forceConsistentCasingInFileNames": true,            
    /* Type Checking */
    "strict": true,                                      
    "noImplicitAny": true,                              
    "noImplicitReturns": true,                           
    "noUncheckedIndexedAccess": true,                    
    "noImplicitOverride": true,                          
    /* Completeness */
    "skipLibCheck": true,

                              
  }
}
